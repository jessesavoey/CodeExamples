ALTER PROC [dbo].[Example_DeleteById]
		@Id int
/*

DECLARE
	@Id int = 0

EXECUTE dbo.Subcontractors_DeleteById @Id

*/

AS

BEGIN

	DELETE

	FROM dbo.Subcontractors
	WHERE Id = @Id

END
